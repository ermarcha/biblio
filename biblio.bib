@InProceedings{Shahrokni04a,
  author =	 {Shahrokni, A. and Drummond, T. and Fua, P.},
  title =	 {Texture Boundary Detection for Real-Time Tracking},
  booktitle =	 {European Conf. on Computer Vision, ECCV'04, LNCS
                  3022},
  pages =	 {566-577},
  year =	 2004,
  pdf =		 {
                  http://springerlink.metapress.com/app/home/contribution.asp?wasp=bec67x4b1g4vpp9e77by&referrer=parent&backto=issue,44,46;journal,83,1636;linkingpublicationresults,1:105633,1},
  Volume =	 2,
  address =	 {Prague, Czech Republic},
  month =	 may,
  annote =	 {We propose an approach to texture boundary detection
                  that only requires a line-search in the direction
                  normal to the edge. It is therefore very fast and
                  can be incorporated into a real-time 3\x{2013}D pose
                  estimation algorithm that retains the speed of those
                  that rely solely on gradient properties along object
                  contours but does not fail in the presence of highly
                  textured object and clutter. This is achieved by
                  correctly integrating probabilities over the space
                  of statistical texture models. We will show that
                  this rigorous and formal statistical treatment
                  results in good performance under demanding
                  circumstances }
}

