@Article{Marchand05a,
  author =	 {Marchand, E. and Chaumette, F.},
  title =	 {Feature tracking for visual servoing purposes},
  journal =	 {Robotics and Autonomous Systems},
  year =	 2005,
  volume =	 52,
  number =	 1,
  pages =	 {53-70},
  doi =		 {http://dx.doi.org/10.1016/j.robot.2005.03.009},
