  author =	 {D. Thalmann and O. Renault and N. Magnenat-Thalmann},
  title =	 {A vision-based approach to behavioural animation},
  journal =	 {The Journal of Visualization and Computer Animation},
  volume =	 1,
  number =	 1,
  pages =	 {18--21},
  month =	 aug,
  year =	 1990,
  annote =	 {This paper presents an innovative way of animating
                  actors at a high level based on the concept of
                  synthetic vision. The objective is simple: to create
                  an animation involving a synthetic actor
                  automatically moving in a corridor avoiding objects
                  and other synthetic actors. To simulate this
                  behavior, each synthetic actor uses a synthetic
                  vision as its perception of the world and so as the
                  unique input to its behavioral model. This model is
                  based on the concept of displacement local automata
                  (DLA), which is similar to the concept of a script
                  for natural language processing. A DLA is an
                  algorithm that can deal with a specific
                  environment. Two DLAs, called `follow-the-corridor'
                  and `avoid-the-obstacle', are described in detail.}
}

