@Article{Sharma97a,
  author =	 {Sharma, R. and Sutanto, H.},
  title =	 {A framework for robot motion planning with sensor
                  constraints},
  journal =	 ieee-tra,
  year =	 1997,
  abstract =	 {Visual feedback can play a crucial role in a dynamic
                  robotic task such as the interception of a moving
                  target. To utilize the feedback effectively, there
                  is a need to develop robot motion planning
                  techniques that also take into account properties of
                  the sensed data. We propose a motion planning
                  framework that achieves this with the help of a
                  space called the perceptual control manifold (PCM)
                  defined on the product of the robot configuration
                  space and an image-based feature space. We show how
                  the task of intercepting a moving target can be
                  mapped to the PCM, using image feature trajectories
                  of the robot end-effector and the moving
                  target. This leads to the generation of motion plans
                  that satisfy various constraints and optimality
                  criteria derived from the robot kinematics, the
                  control system, and the sensing mechanism. Specific
                  interception tasks are analyzed to illustrate this
                  vision-based planning technique },
  volume =	 13,
  number =	 1,
  pages =	 {61-73},
  month =	 feb
}

