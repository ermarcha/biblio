@Article{Simon02b,
  author =	 {Simon, G. and Berger, M.-O.},
  title =	 {Pose Estimation for Planar Structures},
  journal =	 {IEEE Computer Graphics and Applications},
  year =	 2002,
  volume =	 22,
  number =	 6,
  pages =	 {46-53},
  month =	 nov,
  pdf =		 {http://computer.org/cga/cg2002/g6toc.htm},
  abstract =	 {Tracking, or camera pose determination, is the main
                  technical challenge in creating augmented
                  reality. This article describes an efficient and
                  reliable method for real-time camera tracking. This
                  method is based on the knowledge of a piecewise
                  planar structure in the scene and requires neither
                  markers nor sensors to recover the viewpoint. The
                  system yields results comparable in accuracy with
                  full structure-and-motion methods but with better
                  reliability. Results are presented, demonstrating
                  tracking for indoors and outdoor urban scenes. }
}

