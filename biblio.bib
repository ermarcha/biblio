@InProceedings{Masson03b,
  author =	 {Masson, L. and Jurie, F. and Dhome, M.},
  title =	 {Contour/Texture Approach for Visual Tracking},
  booktitle =	 { 13th Scandinavian Conf. on Image Analysis, SCIA
                  2003},
  pages =	 {661-668},
  publisher =	 {Springer},
  series =	 {Lecture Notes in Computer Science},
  volume =	 2749,
  year =	 2003,
  pdf =
                  {http://springerlink.metapress.com/app/home/contribution.asp?wasp=6ea5d38qwk1wvnbdtav0&referrer=parent&backto=issue,88,152;journal,330,1619;linkingpublicationresults,1:105633,1},
  abstract =	 { In this article, the problem of real-time hybrid
                  contour/texture tracking for planar objects is
                  addressed. On one hand, a lot of methods have been
                  proposed to track objects from their contours. On
                  the other hand, numerous other tracking algorithms
                  deal with texture. In real situations, objects tan
                  unfortunately rarely be divided so
                  clearly. Therefore, an hybrid tracking approach,
                  able to mix texture and contour information, appears
                  to be very useful. The proposed approach is very
                  simple and efficient. It is based on image
                  differences, which are the differences between
                  object aspects in the image and aspects predicted
                  using a parametric transformation model. Knowing a
                  difference image, the proposed algorithm only need a
                  matrix multiplication to estimate motion
                  parameters. This is possible due to the use of an
                  off-line learning stage.}
}

