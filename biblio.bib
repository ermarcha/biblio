  title =	 {DARP: Depth-Assisted Rectification of Patches},
  booktitle =	 {IAPR Int. Conf. on Pattern Recognition},
  year =	 2012,
  address =	 {Tsukuba, Japan},
  month =	 nov,
  abstract =	 {This paper presents a method named Depth-Assisted
                  Rectification of Patches (DARP), which exploits
                  depth information available in RGB-D consumer
                  devices to improve keypoint matching of
                  perspectively distorted images. This is achieved by
                  generating a projective rectification of a patch
                  around the keypoint, which is normalized with
                  respect to perspective distortions and scale. The
                  DARP method runs at interactive rates and can be
                  used with any local feature detector and
                  descriptor. The evaluation of the method with planar
                  and non-planar scenes shows that DARP can obtain
                  better results than existing keypoint matching
                  approaches. }
}

