@InProceedings{Pressigout06a,
  author =	 {Pressigout, M. and Marchand, E.},
  title =	 {Fusion de primitives visuelles pour le suivi 3D
                  temps-réel},
  booktitle =	 {15e congrès francophone AFRIF-AFIA Reconnaissance
                  des Formes et Intelligence Artificielle, RFIA'06},
  year =	 2006,
  address =	 {Tours, France},
  pdf =
                  {http://www.irisa.fr/lagadic/pdf/2006_rfia_pressigout.pdf},
  month =	 {jan},
  type =	 nationale,
  abstract =	 {Cet article présente une méthode de suivi 3D basé
                  modèle, précise et robuste. L'information apportée
                  par le motif de l'objet est intégrée à un algorithme
                  classique de calcul de pose reposant sur ses
                  contours de façon à obtenir un résultat plus
                  fiable. La robustesse du suivi est assurée par
                  l'utilisation des M-estimateurs dans le processus
                  d'estimation et un modèle multi-échelle des
                  motifs. L'approche a été validée sur des séquences
                  vidéos ainsi que sur des applications en ligne de
                  Réalité Augmentée. Ces résultats montrent que la
                  méthode proposée permet une localisation à la
                  cadence vidéo tout en étant robuste aux larges
                  mouvements et à des environnements réalistes.}
}

@InProceedings{Pressigout06b,
  author =	 {Pressigout, M. and Marchand, E.},
  title =	 {Real-time 3D Model-Based Tracking: Combining Edge
                  and Texture Information},
  booktitle =	 {IEEE Int. Conf. on Robotics and Automation, ICRA'06},
  year =	 2006,
  address =	 {Orlando, Florida},
  pages =	 {2726-2731},
  pdf =
                  {http://www.irisa.fr/lagadic/pdf/2006_icra_pressigout.pdf},
  month =	 {mai},
  abstract =	 {This paper proposes a real-time, robust and
                  efficient 3D model-based tracking algorithm. A non
                  linear minimization approach is used to register 2D
                  and 3D cues for monocular 3D tracking. The
                  integration of texture information in a more
                  classical non-linear edge-based pose computation
                  highly increases the reliability of more
                  conventional edge-based 3D tracker.  Robustness is
                  enforced by integrating a M-estimator into the
                  minimization process via an iteratively re-weighted
                  least squares implementation. The method presented
                  in this paper has been validated on several video
                  sequences as well as in visual servoing experiments
                  considering various objects.  Results show the
                  method to be robust to large motions and textured
                  environments.}
}

