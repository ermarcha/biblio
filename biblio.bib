@Article{Denavit55,
  author =	 {J. Denavit and R. S. Hartenberg},
  year =	 1955,
  title =	 {A Kinematic Notation for Lower-Pair Mechanisms Based
                  on Matrices},
  journal =	 {J. Appl. Mechanics},
  month =	 jun,
  volume =	 22,
  pages =	 {215--221},
  asbtract =	 {Reuleaux [1875] attempted to formalize a symbolic
                  notation for spatial mechanisms but he did not
                  include all the variables needed for a complete
                  kinematic description of the mechanism, and it is in
                  this classic paper that a kinematically complete
                  symbolic notation is evolved. Spatial mechanisms
                  with only lower pairs (surface contact) are
                  considered. The substance of this notation continues
                  to this day. The main contribution of this paper
                  lies in the system for selecting successive link
                  coordinates which makes the transformation across a
                  joint the function of only four parameters. This is
                  a reflection of the fact that the spatial
                  relationship between any two lines in space can be
                  expressed as a sequence of two screws (a screw =
                  translation and rotation about the same line). For
                  easy computer implementation, homogeneous
                  coordinates and 4x4 matrices are used to represent
                  the coordinate transformations across each
                  joint. The matrices for prismatic (sliding) and
                  revolute rotating joints are formulated. The
                  Denavit-Hartenberg link coordinate assignments were
                  refined further by Sheth and Uicker [1971] who
                  extended the notation to cover the entire range of
                  mechanisms including higher pairs (point or line
                  contact).},
}

