@INPROCEEDINGS{Ming01a,
  AUTHOR =	 {Li, M.},
  TITLE =	 {Correspondence Analysis Between The Image Formation
                  Pipelines of Graphics and Vision},
  BOOKTITLE =	 {Spanish Symposium on Pattern Recognition and Image
                  Analysis},
  YEAR =	 2001,
  PAGES =	 {187--192},
  ADDRESS =	 {Benicasim, Castellón, Spain},
  MONTH =	 may,
  abstract =	 {Augmented Reality can merge the computer-generated
                  objects into the real image or video sequences, one
                  key issue is to keep the geometric consistency
                  between the real and virtual objects. This is
                  achieved by using the camera parameters recovered
                  from the real image to render the virtual objects.
                  Camera parameters' recovery is a basic problem in
                  the area of computer vision. On the other hand,
                  rendering is the major concern of computer
                  graphics. Ususally in these two areas different
                  image formation pipelines are used. This paper
                  presents a side-by-side comparison between the
                  pipelines, in order to make it easier to setup the
                  recovered camera parameters to do the rendering for
                  AR applications. Some experiment results are shown
                  to verify the correctness of the analysis.}
}

